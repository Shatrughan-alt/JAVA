import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int arr[]={1};
	    int max=Integer.MIN_VALUE;
	    int min=0;
	    for(int i=0;i<arr.length;i++){
	        min =min+arr[i];
	        if(max<min)
	        max=min;
	        if(min<0)
	        min=0;
	    }
		System.out.println(max);
	}
}
